<% cssString = calcul_z_index(item.position) %>
<% cssString += build_css_string(item.data['css']) %>
<% if item.data['style'] %>
  <% cssString +=  build_css_string(item.data['style']) %>
<% end %>

<div class='item_warp' id='<%= item.uuid %>' style='<%= "#{cssString} position:absolute;" %>'>
  <% case item.media_type %>
    <% when 'text' %>
      <%= render 'items/statics/text', :item => item %>
    <% when 'widget' %>
      <%= render 'items/statics/widget', :item => item %>
    <% when 'image' %>
      <%= render 'items/statics/image', :item => item %>
    <% when 'iframe' %>
      <%= render 'items/statics/iframe', :item => item %>
    <% when 'html' %>
      <%= render 'items/statics/html', :item => item %>
  <% end %>
</div>      