%h2 Log in

- semantic_form_for :user, :url => session_path(@user) do |f|
  - f.inputs do
    = f.input :email, :required => false
    = f.input :password, :required => false
    = f.input :remember_me, :as => :boolean, :required => false
  
  - f.buttons do
    = f.commit_button :login

= render "shared/devise_links"