      <fieldset>
        <div>
          <label for="property_font_family">Font</label>
          <input  id="property_font_family" type="text" data-property="fontFamily" value="inherit" />
        </div>
        
        <div>
          <label for="property_font_size">Size</label>
          <input  id="property_font_size" type="range" min="0.6" max="4" step="0.1" data-property="fontSize" value="1" />
        </div>
        
        <div>
          <label for="property_font_weight">Bold</label>
          <input  id="property_font_weight" type="checkbox" data-property="fontWeight" value="bold" />
        </div>
        
        <div>
          <label for="property_font_style">Italic</label>
          <input  id="property_font_style" type="checkbox" data-property="fontStyle" value="italic" />
        </div>
        
        <div>
          <label for="property_text_decoration">Underline</label>
          <input  id="property_text_decoration" type="checkbox" data-property="textDecoration" value="underline" />
        </div>
        
        <div>
          <label for="property_text_align">Align</label>
          <input name="property_text_align" id="property_text_align_left" type="radio" data-property="textAlign" value="left" />
          <input name="property_text_align" id="property_text_align_center" type="radio" data-property="textAlign" value="center" />
          <input name="property_text_align" id="property_text_align_right" type="radio" data-property="textAlign" value="right" />
        </div>
        
        <div>
          <label for="property_word_spacing">Word Spacing</label>
          <input  id="property_word_spacing" type="range" min="0" max="2" step="0.05" data-property="wordSpacing" value="0" />
        </div>
        
        <div>
          <label for="property_letter_spacing">Letter Spacing</label>
          <input  id="property_letter_spacing" type="range" min="-0.2" max="1.4" step="0.02" data-property="letterSpacing" value="0" />
        </div>
        
        <div>
          <label for="property_text_shadow">Text Shadow</label>
          <input  id="property_text_shadow" type="text" data-property="textShadow" value="" />
        </div>
			</fieldset>
			
			<fieldset>
        <div>
          <label for="property_color">color</label>
          <input id="property_color" type="text" data-type="cssvalue" data-property="color" autocomplete="on" value="" />
        </div>
      </fieldset>
      
      <fieldset>
